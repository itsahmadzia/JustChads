<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.ScatterChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="686.0" prefWidth="1140.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.focus.ScatterChartController">
   <children>
      <ScatterChart fx:id="scatterchart" layoutX="7.0" layoutY="149.0" prefHeight="508.0" prefWidth="1070.0" title="Scatter Chart">
        <xAxis>
          <CategoryAxis label="Name" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis label="Streak" side="LEFT" />
        </yAxis>
      </ScatterChart>
      <ChoiceBox fx:id="mychoicebox" layoutX="14.0" layoutY="58.0" prefHeight="25.0" prefWidth="205.0" />
      <Button fx:id="generate" layoutX="14.0" layoutY="98.0" mnemonicParsing="false" onAction="#onclick" text="generate" />
      <Button fx:id="Back" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
   </children>
</AnchorPane>
