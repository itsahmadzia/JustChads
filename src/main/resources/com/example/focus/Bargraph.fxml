<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="649.0" prefWidth="919.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.focus.BarGraphController">
   <children>
      <BarChart fx:id="barchart" layoutX="8.0" layoutY="123.0" prefHeight="485.0" prefWidth="866.0" title="Bar Chart">
        <xAxis>
          <CategoryAxis label="Name" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis label="Streaks" side="LEFT" />
        </yAxis>
      </BarChart>
      <ChoiceBox fx:id="mychoicebox" layoutX="14.0" layoutY="53.0" prefHeight="25.0" prefWidth="186.0" />
      <Button fx:id="generate" layoutX="14.0" layoutY="91.0" mnemonicParsing="false" onAction="#onclick" text="generate" />
      <Button fx:id="back" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
   </children>
</AnchorPane>
