<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.AreaChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="706.0" prefWidth="1383.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.focus.AreaChartController">
   <children>
      <ChoiceBox fx:id="mychoicebox" layoutX="9.0" layoutY="54.0" prefWidth="150.0" />
      <Button fx:id="generate" layoutX="20.0" layoutY="100.0" mnemonicParsing="false" onAction="#onclick" text="generate" />
      <AreaChart fx:id="areachart" layoutX="20.0" layoutY="125.0" prefHeight="575.0" prefWidth="1131.0" title="Area Chart">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </AreaChart>
      <Button fx:id="back" layoutX="20.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
   </children>
</AnchorPane>
